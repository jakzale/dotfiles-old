" Using only a subset of provided packages
let g:spf13_bundle_groups = ['general', 'programming']

" Disable views (check this for now)
let g:spf13_no_views = 1

" Set the leader to space
let g:spf13_leader = "\<Space>"
" Keep the local leader to same for now
let g:spf13_localleader=g:spf13_leader

" Disable spf13 WrapRelativeMotion
let g:spf13_no_wrapRelMotion = 1

" I rarely use tabs
let g:spf13_no_fastTabs = 1

